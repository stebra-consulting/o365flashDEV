<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
  <CustomAction Id="4c4279cf-dc04-4f21-9a3b-98eb84905ef7.DEVPublishButton"
                RegistrationType="List"
                RegistrationId="{$ListId:Lists/Nyhetslista;}"
                Location="CommandUI.Ribbon"
                Sequence="10001"
                Title="Invoke &apos;DEVPublishButton&apos; action">
    <CommandUIExtension>
      <!-- 
      Update the UI definitions below with the controls and the command actions
      that you want to enable for the custom action.
      -->
      <CommandUIDefinitions>
        <CommandUIDefinition Location="Ribbon.ListItem.Actions.Controls._children">
          <Button Id="Ribbon.ListItem.Actions.DEVPublishButtonButton"
                  Alt="DEV PublishButton"
                  Sequence="100"
                  Command="Invoke_DEVPublishButtonButtonRequest"
                  LabelText="DEV PublishButton"
                  TemplateAlias="o1"
                  Image32by32="_layouts/15/images/placeholder32x32.png"
                  Image16by16="_layouts/15/images/placeholder16x16.png" />
        </CommandUIDefinition>
      </CommandUIDefinitions>
      <CommandUIHandlers>
        <CommandUIHandler Command="Invoke_DEVPublishButtonButtonRequest"
                          CommandAction="~remoteAppUrl/Publish/?{StandardTokens}&amp;SPListItemId={SelectedItemId}&amp;SPListId={SelectedListId}"/>
      </CommandUIHandlers>
    </CommandUIExtension >
  </CustomAction>
</Elements>
